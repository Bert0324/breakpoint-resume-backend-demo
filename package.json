{
  "name": "express-playground",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'yarn start'",
    "start": "ts-node ./src/index.ts",
    "pro": "pm2 start ./pm2.config.js",
    "build": "rimraf app && tsc -p tsconfig.json"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  }
}
